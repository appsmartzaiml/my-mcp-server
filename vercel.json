{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/mcp",
            "methods": [
                "POST"
            ],
            "dest": "dist/index.js"
        },
        {
            "src": "/.well-known/openai-apps-challenge",
            "methods": [
                "GET"
            ],
            "dest": "dist/index.js"
        },
        {
            "src": "/mcp.json",
            "methods": [
                "GET"
            ],
            "dest": "dist/index.js"
        },
        {
            "src": "/UpdatedRFMIcon.png",
            "dest": "/public/UpdatedRFMIcon.png"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}